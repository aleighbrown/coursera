from PatternToNumber import *

def ComputingFrequencies(text,k):
    freqList = [0] * (4**k - 1)
    for i in range(0, len(text) - k + 1):
        patt = text[i : i + k]
        j = PatternToNumber(patt)

        freqList[j] = freqList[j] + 1
    return freqList
        
r = ComputingFrequencies('ACGCGGCTCTGAAA',2)
